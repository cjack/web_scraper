//						continue;
//					}

					charset = "windows-1252";
					
					int j = 1;
					
					while(j <= 5) {
						
						detailsPageHtml = fetchPageContent(detailsPageURL);
						//detailsPageHtml = fetchPageContent("http://www.efaucets.com/detail.asp?Product_Id=THU9264");	
						
						if(detailsPageHtml == null){
							j++;
