	private String skloniSaKraja(String niska, String karakter) {
		
		niska = niska.trim();
		
		int duzinaNiske = niska.length();
		
		int pozKaraktera = niska.lastIndexOf(karakter);
		
		if(duzinaNiske-1 == pozKaraktera) {
			niska = niska.substring(0, niska.length()-1);
		}
		
		return niska;
	}

