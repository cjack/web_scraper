package bots;

public class Game {
	
	private String url;
	private String name;
	private String type;
	private float value;
	private float lastCheck;
	private boolean alerted;
	private String note;
	
	public Game (String url, String name, String type, float value) {
		this.url = url;
		this.name = name;
		this.type = type;
		this.value = value;
		this.lastCheck = -1;
		this.alerted = false;
	}
	
	public String getUrl() {
		return url;
	}
	public void setUrl(String url) {
		this.url = url;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public boolean isAlerted() {
		return alerted;
	}
	public void setAlerted(boolean alerted) {
		this.alerted = alerted;
	}
	public float getLastCheck() {
		return lastCheck;
	}
	public void setLastCheck(float lastCheck) {
		this.lastCheck = lastCheck;
	}
	public float getValue() {
		return value;
	}
	public void setValue(float value) {
		this.value = value;
	}

	public String getNote() {
		return note;
	}

	public void setNote(String note) {
		this.note = note;
	}
	
	

}
